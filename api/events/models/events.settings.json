{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "events",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "venue": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "performers": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "events"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "genre": {
      "type": "string"
    },
    "time": {
      "type": "string",
      "required": true
    }
  }
}
